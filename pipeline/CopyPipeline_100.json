{
	"name": "CopyPipeline_100",
	"properties": {
		"activities": [
			{
				"name": "Copy_100",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "synapsedvdemo/Data/marvel_search/"
					},
					{
						"name": "Destination",
						"value": "synapsedvdemo/Stage1/marvel_hero/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"additionalColumns": [
							{
								"name": "FilePath",
								"value": "$$FILEPATH"
							}
						],
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "*",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "MergeFiles"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"validateDataConsistency": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['aliases'][0]"
								},
								"sink": {
									"name": "aliases']["
								}
							},
							{
								"source": {
									"path": "$['authors'][0]"
								},
								"sink": {
									"name": "authors']["
								}
							},
							{
								"source": {
									"path": "$['description']"
								},
								"sink": {
									"name": "description"
								}
							},
							{
								"source": {
									"path": "$['images']['background']"
								},
								"sink": {
									"name": "background"
								}
							},
							{
								"source": {
									"path": "$['images']['thumbnail']"
								},
								"sink": {
									"name": "thumbnail"
								}
							},
							{
								"source": {
									"path": "$['mainColor']"
								},
								"sink": {
									"name": "mainColor"
								}
							},
							{
								"source": {
									"path": "$['name']"
								},
								"sink": {
									"name": "name"
								}
							},
							{
								"source": {
									"path": "$['partners'][0]"
								},
								"sink": {
									"name": "partners']["
								}
							},
							{
								"source": {
									"path": "$['powers'][0]"
								},
								"sink": {
									"name": "powers']["
								}
							},
							{
								"source": {
									"path": "$['ranking']['comicCount']"
								},
								"sink": {
									"name": "comicCount"
								}
							},
							{
								"source": {
									"path": "$['ranking']['eventCount']"
								},
								"sink": {
									"name": "eventCount"
								}
							},
							{
								"source": {
									"path": "$['ranking']['pageviewCount']"
								},
								"sink": {
									"name": "pageviewCount"
								}
							},
							{
								"source": {
									"path": "$['ranking']['serieCount']"
								},
								"sink": {
									"name": "serieCount"
								}
							},
							{
								"source": {
									"path": "$['ranking']['storyCount']"
								},
								"sink": {
									"name": "storyCount"
								}
							},
							{
								"source": {
									"path": "$['secretIdentities'][0]"
								},
								"sink": {
									"name": "secretIdentities']["
								}
							},
							{
								"source": {
									"path": "$['species'][0]"
								},
								"sink": {
									"name": "species']["
								}
							},
							{
								"source": {
									"path": "$['superName']"
								},
								"sink": {
									"name": "superName"
								}
							},
							{
								"source": {
									"path": "$['teams'][0]"
								},
								"sink": {
									"name": "teams']["
								}
							},
							{
								"source": {
									"path": "$['urls']['marvel']"
								},
								"sink": {
									"name": "marvel"
								}
							},
							{
								"source": {
									"path": "$['urls']['wikipedia']"
								},
								"sink": {
									"name": "wikipedia"
								}
							},
							{
								"source": {
									"path": "$['File']"
								},
								"sink": {
									"name": "File"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_100",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_100",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}