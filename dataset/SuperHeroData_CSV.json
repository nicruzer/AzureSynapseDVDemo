{
	"name": "SuperHeroData_CSV",
	"properties": {
		"linkedServiceName": {
			"referenceName": "azuredvdemo-WorkspaceDefaultStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"TableDirectory": {
				"type": "string"
			},
			"FileName": {
				"type": "string"
			}
		},
		"folder": {
			"name": "External"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "Data/@{dataset().TableDirectory}",
					"type": "Expression"
				},
				"fileSystem": "synapsedvdemo"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}