{
	"name": "MoveMarvelSearch_JSON",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "MarvelSearchJSON",
						"type": "DatasetReference"
					},
					"name": "marvelSearchJsonFiles"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Parquet1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "flatten1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          aliases as string[],",
				"          authors as string[],",
				"          description as string,",
				"          images as (background as string, thumbnail as string),",
				"          mainColor as string,",
				"          name as string,",
				"          partners as string[],",
				"          powers as string[],",
				"          ranking as (comicCount as integer, eventCount as integer, pageviewCount as integer, serieCount as integer, storyCount as integer),",
				"          secretIdentities as string[],",
				"          species as string[],",
				"          superName as string,",
				"          teams as string[],",
				"          urls as (marvel as string, wikipedia as string)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false,",
				"     rowUrlColumn: 'FileName',",
				"     documentForm: 'arrayOfDocuments') ~> marvelSearchJsonFiles",
				"marvelSearchJsonFiles foldDown(unroll(aliases),",
				"     mapColumn(",
				"          aliases,",
				"          description,",
				"          name,",
				"          superName,",
				"          FileName",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1",
				"flatten1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}