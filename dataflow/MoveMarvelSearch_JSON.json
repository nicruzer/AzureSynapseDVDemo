{
	"name": "MoveMarvelSearch_JSON",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "MarvelSearchJSON",
						"type": "DatasetReference"
					},
					"name": "marvelSearchJsonFiles"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          aliases as string[],",
				"          authors as string[],",
				"          description as string,",
				"          images as (background as string, thumbnail as string),",
				"          mainColor as string,",
				"          name as string,",
				"          partners as string[],",
				"          powers as string[],",
				"          ranking as (comicCount as integer, eventCount as integer, pageviewCount as integer, serieCount as integer, storyCount as integer),",
				"          secretIdentities as string[],",
				"          species as string[],",
				"          superName as string,",
				"          teams as string[],",
				"          urls as (marvel as string, wikipedia as string)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false,",
				"     rowUrlColumn: 'FileName',",
				"     documentForm: 'arrayOfDocuments') ~> marvelSearchJsonFiles",
				"marvelSearchJsonFiles select(skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     keys:['name','FileName','superName'],",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: true,",
				"     saveOrder: 1) ~> sink1"
			]
		}
	}
}